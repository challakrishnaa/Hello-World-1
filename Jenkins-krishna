pipeline {
    agent any
    environment {
        Path=/usr/share/maven:$Path
    }
    steps {
        stage ("git clone") {
            steps {
              git credentialsId: 'pipeline', url: 'https://github.com/challakrishnaa/Hello-World-1.git'
            }
        }
        stage ("build") {
            steps {
                sh "mvn clean package"
            }
        }
    }

}


